{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      },{
        "functions": {
    "api/function-name": {
      "memory": 512,
      "maxDuration": 30 
    }
  }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.js"
      }
    ]
  }
  